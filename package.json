{
  "name": "codeflow-autocomplete",
  "version": "1.0.0",
  "description": "Production-grade C++ autocomplete engine with Trie + AST + ML ranking",
  "main": "main.js",
  "homepage": "https://github.com/yourusername/codeflow-autocomplete",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/codeflow-autocomplete"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "postinstall": "npm run build",
    "install-deps": "npm install && cd backend && npm install",
    "build": "cd backend && node-gyp configure build && cd .. && npm run build:frontend",
    "build:frontend": "cd frontend && npm install && npm run build",
    "build:backend": "cd backend && cmake . && make",
    "start": "electron .",
    "dev:frontend": "cd frontend && npm start",
    "dev:backend": "cd backend && node-gyp rebuild --debug",
    "test": "jest --coverage && npm run test:cpp",
    "test:cpp": "cd tests && cmake . && ctest --output-on-failure",
    "lint": "eslint frontend/src --fix",
    "format": "prettier --write \"frontend/src/**/*.{js,jsx,css}\"",
    "prepackage": "npm run build",
    "package": "electron-builder",
    "dist": "npm run build && electron-builder --publish=always"
  },
  "keywords": [
    "autocomplete",
    "C++",
    "Trie",
    "AST",
    "LSP",
    "IntelliSense",
    "VSCode",
    "Monaco",
    "Electron"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@monaco-editor/react": "^4.5.0",
    "axios": "^1.6.0",
    "node-addon-api": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "concurrently": "^8.2.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.6.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "jest": "^29.7.0",
    "node-gyp": "^9.4.0",
    "node-gyp-build": "^4.7.0",
    "prettier": "^3.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0"
  },
  "binary": {
    "module_name": "codeflow_native",
    "module_path": "./dist/",
    "remote_path": "./releases/v{version}/",
    "package_name": "{module_name}-v{version}-{platform}-{arch}.tar.gz"
  }
}